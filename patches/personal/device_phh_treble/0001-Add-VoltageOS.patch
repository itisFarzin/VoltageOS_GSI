From ce3ce419f10116a48b09efa8a7938609470ce57f Mon Sep 17 00:00:00 2001
From: Farzin Kazemzadeh <itisFarzin@proton.me>
Date: Sun, 7 Dec 2025 05:04:50 +0000
Subject: [PATCH 1/4] Add VoltageOS

Signed-off-by: Farzin Kazemzadeh <itisFarzin@proton.me>
---
 AndroidProducts.mk |  2 ++
 voltage_gsi.mk     | 26 ++++++++++++++++++++++++++
 2 files changed, 28 insertions(+)
 create mode 100644 AndroidProducts.mk
 create mode 100644 voltage_gsi.mk

diff --git a/AndroidProducts.mk b/AndroidProducts.mk
new file mode 100644
index 0000000..99c6f1b
--- /dev/null
+++ b/AndroidProducts.mk
@@ -0,0 +1,2 @@
+PRODUCT_MAKEFILES := \
+	$(LOCAL_DIR)/voltage_gsi.mk
diff --git a/voltage_gsi.mk b/voltage_gsi.mk
new file mode 100644
index 0000000..9399786
--- /dev/null
+++ b/voltage_gsi.mk
@@ -0,0 +1,26 @@
+$(call inherit-product, device/phh/treble/base.mk)
+$(call inherit-product, vendor/voltage/config/common_full_phone.mk)
+
+PRODUCT_NAME := treble_arm64_bvN
+PRODUCT_DEVICE := tdgsi_arm64_ab
+PRODUCT_BRAND := google
+PRODUCT_SYSTEM_BRAND := google
+PRODUCT_MODEL := VoltageOS vanilla
+
+# Overwrite the inherited "emulator" characteristics
+PRODUCT_CHARACTERISTICS := device
+
+# Disable adb secure
+WITH_ADB_INSECURE := true
+
+# GSIs don't have kernel
+TARGET_NO_KERNEL := true
+
+# Enable face unlock
+TARGET_FACE_UNLOCK_SUPPORTED := true
+
+# Bootanimation
+TARGET_BOOT_ANIMATION_RES := 1080
+
+# Disable INSTALLED_KERNEL_TARGET warning
+PRODUCT_OTA_ENFORCE_VINTF_KERNEL_REQUIREMENTS := false
